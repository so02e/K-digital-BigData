   "source": [
    "# 정수 n개가 주어졌을 때 n개의 합을 구하는 함수\n",
    "\n",
    "def solve(*ints):\n",
    "    sum = 0\n",
    "    for num in ints :\n",
    "        sum += num\n",
    "    return sum\n",
    "\n",
    "\n",
    "print(solve(1,2))\n",
    "print(solve(5,3,1))\n"
